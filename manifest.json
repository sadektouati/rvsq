{
    "manifest_version": 3,
    "name": "RVSQ Extension",
    "version": "1.0",
    "description": "An extension that helps administrative agents.",
    "permissions": ["activeTab"],
    "icons": {
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://rvsq.gouv.qc.ca/QuickSearch*"],
            "css": ["s_search_appointement.css"],
            "js": ["s_search_appointement.js"]
        },
        {
            "matches": ["https://rvsq.gouv.qc.ca/prendrerendezvous/*"],
            "js": ["s_book_appointement.js"]
        }
    ]
  }
  